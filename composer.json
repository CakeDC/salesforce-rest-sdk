{
    "name": "codegenus/salesforce-rest-sdk",
    "description": "An SDK for the Salesforce Rest API",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "jms/serializer": "^3.0",
        "ramsey/uuid": "^4.0",
        "doctrine/collections": "^1.5|^2.0",
        "symfony/cache": "^5.0|^6.0|^7.0",
        "symfony/expression-language": "^5.0|^6.0|^7.0",
        "jms/metadata": "^1.0|^2.0"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Alex Boyce",
            "email": "alex.boyce@advisorsexcel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\Tests\\": ["Tests/"]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
